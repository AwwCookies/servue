        "vue-template-compiler": "^2.6.6"
    },
    "devDependencies": {
        "axios": "^0.18.0",
        "codacy-coverage": "^3.3.0",
        "codecov": "^3.1.0",
        "eslint": "^5.10.0",
        "express": "^4.16.4",
